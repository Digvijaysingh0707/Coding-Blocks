import java.util.Scanner;

public class Main {

   public static int  counting1(int n) {
       int ans=0;
       while(n>0){
           ans++;
           n=n&(n-1);

       }
       return ans;
   }




        public static void main (String[]args){
        int n=13;
            System.out.println(counting1(n));


        }
    }
