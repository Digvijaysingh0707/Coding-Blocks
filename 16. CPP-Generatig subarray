#include <iostream>

using namespace std;

int main()
{int n;
cin>>n;
int a[100];
for(int i =0;i<n;i++){
    cin>>a[i];
}
//Generate all subarray
for(int i =0;i<n;i++){
    for(int j =i;j<n;j++){
        for(int k =i;k<=j;k++){
            cout<<a[i]<<",";
        }
        cout<<endl;
    }
}

    return 0;
}


//JAVA CODE
package com.company;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the value of n: ");
        int n = sc.nextInt();
        int[] arr = new int[n];
        System.out.println("Enter the elements of array: ");
        for(int i =0;i<n;i++){
            arr[i]=sc.nextInt();
        }
       for(int i =0;i<n;i++){
           for(int j=i;j<n;j++){
               //For printing the subArrays
               for(int k =i;k<=j;k++){
                   System.out.print(arr[k]);
               }
               System.out.println();
           }
           
       }


    }

	}


