package Greedy;

import java.security.KeyPair;
import java.util.*;

class q{
     static class Pair{
        private int x;
        private int y;
        public Pair(int x, int y){
            this.x = x;
            this.y = y;
        }
        public int getX() {
            return this.x;
        }
        public int getY() {
            return this.y;
        }
        }
    }
public class Expedition {
    static boolean compare(q.Pair p1,q.Pair p2){
        return p1.f
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n,tx,d,f,D,F,prev=0;
        int t=sc.nextInt();
        while (t-->=1){
            ArrayList<q.Pair> v = new ArrayList<>();
            PriorityQueue<Integer> pq;
            n=sc.nextInt();
            for (int i=0;i<n;i++){
                d=sc.nextInt();
                f=sc.nextInt();
                v.add(new q.Pair(d,f));
            }
            //Logic
            Collections.sort(v,compare);
        }
    }
}


