class Solution

{
     public long count(int S[], int m, int n) 
    { 
        long dp[]= new long[n+1];
        dp[0]=1;
        // Arrays.sort(S);
        for(int i=0;i<m;i++){
            for(int j=S[i];j<dp.length;j++){
                dp[j]+=dp[j-S[i]];
            }
        }
        return dp[n];
    } 
}
