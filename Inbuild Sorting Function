#include <iostream>
#include<algorithm>

using namespace std;

int main()
{

    int a[] ={5,7,2,3,6,8};
    int n = sizeof(a)/sizeof(int);

    sort(a,a+n);
    for(int i =0;i<n;i++){
        cout<<a[i]<<" ";
    }
    return 0;
}






#include <iostream>
#include<algorithm>

using namespace std;
// For sorting in descending Order

bool myCompare(int a,int b){
        return a>b;
}

int main()
{

    int a[] ={5,7,2,3,6,8};
    int n = sizeof(a)/sizeof(int);

    sort(a,a+n,myCompare);
    for(int i =0;i<n;i++){
        cout<<a[i]<<" ";
    }
    return 0;
}

