#include <iostream>

using namespace std;

int main()
{
    int a[10];
    int i;
    int n;
    cin>>n;
    for(int i =0;i<n;i++){
        cin>>a[i];
    }
    int key;
    cin>>key;
    for( i =0;i<n;i++){
        if(key==a[i]){
            cout<<"Elements is found at "<<i<<endl;
            break;
        }

    }
    if(i==n){
        cout<<"The "<<key<<" is not present";
    }

    return 0;
}
