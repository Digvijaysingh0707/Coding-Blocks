       //TC O(logn) 
        Scanner sc = new Scanner(System.in);
        int a=sc.nextInt();
        int n=sc.nextInt();
        int ans=1;
        while (n>0){
            if ((n&1)==1){
                ans=ans*a;
            }
            a=a*a;
            n>>=1;
        }
        System.out.println(ans);
