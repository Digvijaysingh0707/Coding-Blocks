public class bit {
static int countBits(int n){
    int ans=0;
    while (n>0){
        ans+=(n&1);
        n=(n>>1);
    }
    return ans;
}
static int countBitsFast(int n){
    int ans=0;
    while (n>0){
        n=n&(n-1);
        ans++;
    }
    return ans;
}

    public static void main(String[] args) {

        System.out.println(countBitsFast(13));
    }
}
