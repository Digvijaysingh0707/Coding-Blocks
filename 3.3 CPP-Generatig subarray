#include <iostream>

using namespace std;

int main()
{int n;
cin>>n;
int a[100];
for(int i =0;i<n;i++){
    cin>>a[i];
}
//Generate all subarray
for(int i =0;i<n;i++){
    for(int j =i;j<n;j++){
        for(int k =i;k<=j;k++){
            cout<<a[i]<<",";
        }
        cout<<endl;
    }
}

    return 0;
}


//JAVA CODE
package com.company;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the value of n: ");
        int n = sc.nextInt();
        int[] arr = new int[n];
        System.out.println("Enter the elements of array: ");
        for(int i =0;i<n;i++){
            arr[i]=sc.nextInt();
        }
       for(int i =0;i<n;i++){
           for(int j=i;j<n;j++){
               //For printing the subArrays
               for(int k =i;k<=j;k++){
                   System.out.print(arr[k]);
               }
               System.out.println();
           }
           
       }


    }

	}
	
	//Printing the array having maximum sum
	
	package com.company;
import java.util.Arrays;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the value of n: ");
        int n = sc.nextInt();
        int[] arr = new int[n];
        System.out.print("Enter the elements of array: ");
        int left =-1;
        int right =-1;
        for (int i = 0; i < n; i++) {
            arr[i] = sc.nextInt();
        }
        int maxSum = 0;
        int currentSum = 0;
        for (int i = 0; i < n; i++) {
            for (int j = i; j < n; j++) {
                currentSum = 0;
                for (int k = i; k <= j; k++) {
                    currentSum += arr[k];

                }
                if (currentSum > maxSum) {
                    maxSum = currentSum;
                    left = i;
                    right=j;
                }
            }


        }
        System.out.println(maxSum);
        int k;
        for( k =left;k<=right;k++){
            System.out.print(arr[k]+" ");
        }
        //System.out.print(Arrays.toString(arr[k]));
    }
}





